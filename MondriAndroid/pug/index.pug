doctype html
html
    head
        title MondriAndroid
        style.
            @import "styles/css/style.css";
    body
        // <?xml version="1.0" encoding="utf-8"?>
        svg#art1(viewBox='0 0 1000 610' xmlns='http://www.w3.org/2000/svg')

            // JavaScript does the building in here
            // <?xml version="1.0" encoding="utf-8"?>
        svg#feedback(viewBox='0 0 200 80' xmlns='http://www.w3.org/2000/svg')
            style.
                .swatch{stroke-width: 1px; stroke: #000;}
                svg text{
                    font-family: 'Helvetica Neue', sans-serif;
                    font-size: 4px;}
                #orig{font-size: 8px}
            text#orig(x='3' y='10') Title:

            rect#swatch1.swatch(x='3' y='15' width='10' height='10')
            rect#swatch2.swatch(x='3' y='25' width='10' height='10')
            rect#swatch3.swatch(x='3' y='35' width='10' height='10')
            rect#swatch4.swatch(x='3' y='45' width='10' height='10')
            text#hue(x='15' y='22') Color A: 
            text#hue2(x='15' y='32') Color B: 
            text#hue3(x='15' y='42') Color C: 
            text#hue3(x='15' y='52') Color D: 

            text#matchednumbers(x='75' y='50') Matched Number: 
             
            text#hash(x='3' y='70') Hash: 
            text#selectedsymbol(x='75' y='22') Selected symbol: 

        include table

        script(src='scripts/md5.js')
        // MD5 generator
        script(src='scripts/generator.js')
        //script(src='scripts/v2.js')
        // everything below here is SVG library symbols
        include svg-library 
